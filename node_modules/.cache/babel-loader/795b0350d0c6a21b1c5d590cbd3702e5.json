     let question_type = this.question_type;\n\n      if (question_type == 'checkbox') {\n        let data = {\n          question_type: this.question_type,\n          question_text: this.question_type,\n          option_type: this.question_type,\n          profile_id: 1,\n          category_id: 0,\n          privacy: 0,\n          publish_status: 0,\n          no_of_option: this.option_no,\n          no_of_used: 0,\n          no_of_comments: 0,\n          average_rating: 0,\n          no_of_answer: this.checkbox_num.length\n        };\n      }\n    },\n\n    handleImageAdded: function (file, Editor, cursorLocation, resetUploader) {\n      var formData = new FormData();\n      formData.append('image', file);\n      axios({\n        url: `${base_url}/upload`,\n        method: 'POST',\n        data: formData\n      }).then(result => {\n        console.log(result);\n        const base_url = result.data.base_url; // Get url from response\n\n        const image_url = result.data.img_url;\n        const url = base_url + '/' + image_url;\n        Editor.insertEmbed(cursorLocation, 'image', url);\n        resetUploader();\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAoKA,OAAOA,KAAP,MAAkB,OAAlB,C,CACA;;AACA,SAASC,QAAT,QAAyB,oBAAzB;AACA,SAASC,SAAT,QAA0B,aAA1B,C,CACA;;AAEA,eAAe;EACbC,IAAI,EAAE,cADO;EAEbC,UAAU,EAAE;IACVF;EADU,CAFC;;EAMbG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EADF;MAELC,YAAY,EAAE,EAFT;MAGLC,SAAS,EAAE,IAHN;MAILC,iBAAiB,EAAE,IAJd;MAKLC,WAAW,EAAE,CAAC,IAAD,CALR;MAMLC,QAAQ,EAAE,EANL;MAOLC,SAAS,EAAE,CAPN;MAQLC,QAAQ,EAAE,EARL;MASLC,aAAa,EAAE,EATV;MAULC,YAAY,EAAE,EAVT;MAWLC,YAAY,EAAE,IAXT;MAYLC,UAAU,EAAE,KAZP;MAaLC,UAAU,EAAE,KAbP;MAcLC,OAAO,EAAE,CACP;QACEC,EAAE,EAAE,CADN;QAEEC,cAAc,EAAE,CAFlB;QAGEC,GAAG,EAAE,CAHP;QAIEC,KAAK,EAAE;MAJT,CADO;IAdJ,CAAP;EAuBD,CA9BY;;EAgCbC,OAAO,GAAG;IACR,KAAKC,KAAL,GADQ,CAER;EACD,CAnCY;;EAoCbC,WAAW,GAAG;IACZ,KAAKC,WAAL;EACD,CAtCY;;EAuCbC,OAAO,EAAE;IACP,MAAMH,KAAN,GAAc;MACZ,IAA